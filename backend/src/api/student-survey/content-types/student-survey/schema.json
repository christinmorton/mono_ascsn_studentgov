{
  "kind": "collectionType",
  "collectionName": "student_surveys",
  "info": {
    "singularName": "student-survey",
    "pluralName": "student-surveys",
    "displayName": "StudentSurvey",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "required": true
    },
    "Title": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text",
      "required": true
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "survey_question_ids": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::survey-question.survey-question"
    },
    "students": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::student.student"
    },
    "SurveyQuestion": {
      "type": "component",
      "repeatable": true,
      "component": "layout.survey-question"
    }
  }
}
